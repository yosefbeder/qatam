متغير قائمة = [0، 1، 2]
متغير نص = "السلام عليكم"
إطبع(قائمة[0])
إطبع(نص[10])
قائمة[0] = 10
إطبع(قائمة)
حاول {
  نص[1] = "ن"
  إطبع(نص)
} أمسك(الخطأ) {
  إطبع(الخطأ)
}
متغير كائن = { الاسم: "يوسف"، العمر: 16 }
إطبع(كائن.الاسم)
كائن.الاسم = "مصطفى"
إطبع(كائن.الاسم)
كائن.مستوى_الذكاء = "متوسط"
إطبع(كائن.مستوى_الذكاء)
إطبع(كائن["مستوى" + "_" + "الذكاء"])
